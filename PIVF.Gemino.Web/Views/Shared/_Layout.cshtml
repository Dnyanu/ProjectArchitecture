                                            Consent : {{ConsentDetails.Consent}}
                                        </div>
                                    </div>
                                    <div class="col-md-3 col-lg-3">
                                        <div class="form-group">
                                            <select class="form-control " ng-model="ConsentDetails.SelectedUnitID" ng-options="item.ID as item.Description for item in ClinicList" />
                                        </div>
                                    </div>

                                    <div class="col-md-12 col-lg-12" id="printSection1">
                                        <text-angular ng-model="htmlForm" ta-toolbar="[['h1','h2','h3'],['bold','italics', 'underline', 'strikeThrough', 'ul', 'ol', 'undo', 'redo', 'clear', 'justifyLeft', 'justifyCenter', 'justifyRight', 'justifyFull', 'indent', 'outdent'],['checkbox', 'input', 'datepicker','insertVariable']]"></text-angular>
                                    </div>
                                    <div class="clear"></div>
                                    <div class="col-lg-12 col-md-12 page_actions text-right">
                                        <button class="btn btn-primary" type="button" id="" value="" ng-click="SaveConsent()"> Save</button>
                                        <button class="btn btn-primary" ng-print print-element-id="printSection1">Print</button>
                                        <button class="btn btn-default" type="button" id="" value="" ng-click="Cancel()"> Cancel</button>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
                <!-- Accordion End -->
            </div>

        </div>
        <div class="modal fade edit-modal" id="myModal_con" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel">
            <div class="modal-dialog modal-lg" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" id="btnClosePop" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        @*<h4 class="modal-title" id="myModalLabel">
                                {{FileName}} &nbsp;-
                                <span class="femaleDetails"> {{PatientName}} <small>({{MRNO}})</small></span>
                            </h4>*@
                    </div>
                    <div class="modal-body">
                        <div class="row form-horizontal">
                            <div class="text-center" ng-if="extn == 'image'">
                                <img ng-src="{{Image}}" style="width: 61%; height: 61%;" />
                            </div>
                            <div class="wrapper" ng-if="extn != 'image'">
                                <object ng-show="content" data="{{content}}" type="application/pdf" style="width: 100%; height: 400px;"></object>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <div class="row form-horizontal">
                            <div class="col-lg-12 text-right">
                                <button class="btn btn-primary" type="button" id="" value="" ng-show="extn == 'image'" ng-click="SaveReport()">Download</button>
                                <button class="btn btn-primary" type="button" id="" value="" ng-show="extn == 'image'">Print</button>
                                <button class="btn btn-default" type="button" id="" value="" ng-click="Close()">Close</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </script>

    <script type="text/ng-template" id="ShowForm">
        <div class="modal-content" ng-init="updateForm()">
            <div id="consentForm" class="">
                <form sf-schema="schema" sf-form="form" sf-model="model" ng-submit="submitForm(form,model)"></form>
            </div>
        </div>
        <div class="row page_actions text-right">
            <div class="col-lg-12 col-md-12">
                <button class="btn btn-primary " type="button" id="" value="" ng-click="showPDF()"> Print</button>
                <button class="btn btn-default " type="button" id="" value="" ng-click="Cancel()"> Cancel</button>
            </div>
        </div>
    </script>

    @Scripts.Render("~/bundles/angularScripts")
    <script src="~/Scripts/app/app.js"></script>
    <script src="~/Scripts/app/controllers/Authentication/HomeCtlr.js"></script>
    <script src="~/Scripts/app/controllers/Authentication/DefaultPageCtlr.js"></script>
    <script src="~/Scripts/app/services/Common/srvCommon.js"></script>
    <script src="~/Scripts/app/services/Authentication/authService.js"></script>
    <script src="~/Scripts/app/services/Authentication/authInterceptorService.js"></script>
    <script src="~/Scripts/app/services/Authentication/LogInService.js"></script>
    <script src="~/Scripts/app/controllers/Authentication/LayOutController.js"></script>
    <script src="~/Scripts/app/services/Common/CommonService.js"></script>
    <script src="~/Scripts/app/StoreDataFactory.js"></script>
    <script src="~/Scripts/app/controllers/Master/Configuration/UserRoleController.js"></script>
    <script src="~/Scripts/app/services/Master/Configuration/UserRoleService.js"></script>
    <script src="~/Scripts/app/controllers/Master/Configuration/UserController.js"></script>
    <script src="~/Scripts/app/services/Master/Configuration/UserService.js"></script>
    <script src="../Scripts/app/controllers/BreadCum/BreadCumCtrl.js"></script>

    @RenderSection("Scripts", required: false)
    <toasty></toasty>
</body>
</html>